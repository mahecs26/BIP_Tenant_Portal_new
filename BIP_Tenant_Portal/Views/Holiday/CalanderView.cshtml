@model List<Holiday>
@{
    ViewData["Title"] = "Holiday Calendar View";
}
@section Styles {
    <link href="https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.3.1/fullcalendar.min.css" rel="stylesheet" />

}
<div class="row">
    <div class="col-xl-12">
        <div class="card">
            <div class="card-body">
                <div id="calendar"></div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    <script>
        var holidays = @Html.Raw(Json.Serialize(Model));
    </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.2/moment.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.3.1/fullcalendar.min.js"></script>
    <script src="~/assets/js/calander-view.js"></script>
}